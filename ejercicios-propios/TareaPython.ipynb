   "source": [
    "import math\n",
    "def cuad(a,b,c):\n",
    "    r1 = (-b+math.sqrt(b**2-4*a*c))/2*a\n",
    "    r2 = (-b-math.sqrt(b**2-4*a*c))/2*a\n",
    "    return(r1,r2)"
   ]
